{
  "name": "react-ts-i18n",
  "version": "0.1.0",
  "description": "Typed solution for i18n in react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist & rollup --config ./rollup.config.js",
